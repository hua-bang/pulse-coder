{
  "name": "@coder/engine",
  "version": "1.0.0",
  "description": "Plugin-based AI engine for Coder",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./src": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js"
    },
    "./built-in": {
      "types": "./dist/built-in/index.d.ts",
      "import": "./dist/built-in/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.42",
    "@ai-sdk/mcp": "^1.0.19",
    "@ai-sdk/openai": "^3.0.21",
    "@requesty/ai-sdk": "^3.1.0",
    "ai": "^6.0.57",
    "dotenv": "^17.2.3",
    "glob": "^11.0.0",
    "gray-matter": "^4.0.3",
    "yaml": "^2.8.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/node": "^25.0.10",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  }
}