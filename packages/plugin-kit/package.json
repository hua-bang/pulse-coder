{
  "name": "pulse-coder-plugin-kit",
  "version": "0.0.1",
  "description": "Toolkit package for Pulse Coder runtime plugins",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./worktree": {
      "types": "./dist/worktree.d.ts",
      "import": "./dist/worktree.js",
      "require": "./dist/worktree.cjs"
    },
    "./src": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./src/worktree": {
      "types": "./src/worktree/index.ts",
      "import": "./src/worktree/index.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "pulse-coder-engine": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
